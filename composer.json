    "description": "A simple HTTP client for sending HTTP requests and receiving responses based on CURL.",
    "license": "MIT",
    "type": "library",
    "keywords": ["brownie", "http-client", "httpclient", "http", "client", "curl", "receiving responses", "request", "response"],
    "homepage": "https://github.com/prnewsteam/bpm-http-client",
    "version": "0.0.7",
    "authors": [
        {
            "name": "brownie",
            "email": "oss.brownie@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.5",
        "ext-curl": "*",
        "ossbrownie/util": "~0.0.5"
    },
    "require-dev": {
        "phpunit/phpunit": "4.8.35",
        "phpdocumentor/reflection-docblock": "2.0.5",
        "php-coveralls/php-coveralls": "^2.1.0"
    },
    "autoload": {
        "psr-4": {
            "Brownie\\HttpClient\\": "src/Brownie/HttpClient/"
